{
    "name": "organizr-web-app",
    "version": "1.0.0",
    "description": "Web app focused to help people organizing tasks and manage time more efficiently.",
    "author": "Group 02 - Pólo Inácio Monteiro - UNIVESP (Class of 2020)",
    "license": "GPL-3.0-or-later",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "sass src/ && nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch --watchAssets",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@nestjs/common": "^8.1.1",
        "@nestjs/config": "^1.0.2",
        "@nestjs/core": "^8.0.6",
        "@nestjs/passport": "^8.0.1",
        "@nestjs/platform-express": "^8.0.6",
        "@nestjs/sequelize": "^8.0.0",
        "@sendgrid/mail": "^7.6.0",
        "bootstrap": "^5.1.3",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "cookie-session": "^1.4.0",
        "dashify": "^2.0.0",
        "date-fns": "^2.25.0",
        "date-fns-tz": "^1.1.6",
        "express-session": "^1.17.2",
        "form-data": "^4.0.0",
        "google-auth-library": "^7.10.2",
        "googleapis": "^91.0.0",
        "handlebars-helpers": "^0.10.0",
        "handlebars-layouts": "^3.1.4",
        "hbs": "^4.1.2",
        "hbs-utils": "0.0.4",
        "helper-date": "^1.0.1",
        "mailgun.js": "^3.7.2",
        "passport": "^0.5.0",
        "passport-local": "^1.0.0",
        "pg": "^8.7.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.4.0",
        "sass": "^1.43.4",
        "sequelize": "^6.7.0",
        "sequelize-typescript": "^2.1.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^8.1.4",
        "@nestjs/schematics": "^8.0.2",
        "@nestjs/testing": "^8.0.6",
        "@types/bootstrap": "^5.1.6",
        "@types/cookie-session": "^2.0.43",
        "@types/dashify": "^1.0.1",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.4",
        "@types/handlebars-helpers": "^0.5.3",
        "@types/hbs": "^4.0.1",
        "@types/jest": "^27.0.1",
        "@types/node": "^16.7.1",
        "@types/passport-local": "^1.0.34",
        "@types/sequelize": "^4.28.10",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^4.29.2",
        "@typescript-eslint/parser": "^4.29.2",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.1",
        "jest": "^27.0.6",
        "localtunnel": "^2.0.2",
        "prettier": "^2.3.2",
        "supertest": "^6.1.6",
        "ts-jest": "^27.0.5",
        "ts-loader": "^9.2.5",
        "ts-node": "^10.2.1",
        "tsconfig-paths": "^3.10.1",
        "typescript": "^4.3.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "moduleNameMapper": {
            "@/(.*)$": "<rootDir>/$1"
        },
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "engines": {
        "node": "14.x"
    },
    "volta": {
        "node": "14.18.1"
    }
}
